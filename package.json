{
  "name": "julianarodrigues",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite --port 5000 --host 0.0.0.0",
    "build": "npm run prune && tsc && vite build && npm run compress-webp",
    "prune": "ts-prune",
    "compress-webp": "npx imagemin-cli 'build/assets/**/*.webp' --out-dir=build --use=imagemin-mozjpeg --use=imagemin-pngquant",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start-ngrok": "ngrok http 5000",
    "start-public": "concurrently \"yarn start\" \"yarn start-ngrok\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/montserrat": "^5.0.16",
    "@fontsource/great-vibes": "^5.0.17",
    "@googleapis/docs": "^3.0.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@react-oauth/google": "^0.12.1",
    "@types/gapi": "^0.0.47",
    "@types/nodemailer": "^6.4.14",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "formik": "^2.4.5",
    "gapi": "^0.0.3",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-router-dom": "^6.20.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@svgr/rollup": "^8.1.0",
    "@types/axios": "^0.14.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-google-recaptcha": "^2.1.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "ngrok": "^5.0.0-beta.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-preload": "^0.3.1",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-svgr": "^4.2.0"
  }
}
